{
  "name": "follow-coin-bot",
  "version": "0.1.0",
  "description": "Telegram bot for cryptocurrency tracking with Long List and Hot List functionality",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "npm run build && node dist/index.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "health": "curl http://localhost:3000/health || echo 'Health check endpoint not available'"
  },
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "node-cron": "^3.0.3",
    "telegraf": "^4.16.3",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "prisma": "^6.14.0",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "telegram",
    "bot",
    "cryptocurrency",
    "tracking",
    "dexscreener",
    "solana"
  ],
  "author": "",
  "license": "MIT"
}
